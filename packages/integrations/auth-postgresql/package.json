{
    "name": "@o2s/integrations.auth-postgresql",
    "version": "1.0.0",
    "private": true,
    "license": "MIT",
    "exports": {
        "./auth": "./dist/index.js",
        "./auth.updateOrganization": "./dist/auth.updateOrganization.js"
    },
    "files": [
        "dist",
        "prisma",
        ".env.local"
    ],
    "scripts": {
        "prepare": "npx prisma generate && tsc && tsc-alias",
        "init": "dotenv -e .env.local -- prisma migrate deploy",
        "build": "dotenv -e .env.local -- prisma migrate deploy && tsc --preserveWatchOutput && tsc-alias",
        "lint": "tsc --noEmit && eslint . --max-warnings=0",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
        "seed": "dotenv -e .env.local -- tsx prisma/seed.ts"
    },
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    },
    "dependencies": {
        "@auth/prisma-adapter": "^2.11.1",
        "@prisma/client": "^6.19.0",
        "bcryptjs": "^3.0.3",
        "dotenv-cli": "^11.0.0",
        "jsonwebtoken": "^9.0.3",
        "prisma": "^6.19.0",
        "@o2s/framework": "*",
        "@o2s/utils.logger": "*"
    },
    "devDependencies": {
        "@types/jsonwebtoken": "^9.0.10",
        "@o2s/eslint-config": "*",
        "@o2s/lint-staged-config": "*",
        "@o2s/prettier-config": "*",
        "@o2s/typescript-config": "*",
        "concurrently": "^9.2.1",
        "eslint": "^9.39.1",
        "prettier": "^3.7.4",
        "tsc-alias": "^1.8.16",
        "typescript": "^5.9.3"
    },
    "peerDependencies": {
        "next-auth": "^5.0.0-beta.30",
        "zod": "^3.25.4 || ^4.0.0"
    }
}
